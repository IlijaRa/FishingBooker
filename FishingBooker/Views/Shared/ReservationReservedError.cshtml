        Layout = "~/Views/Shared/_LayoutInstructor.cshtml";
    }
}

<hgroup>
    <h1 class="text-warning">Everything is ok, but</h1>
    <h2 class="text-warning">This reservation is already reserved so it cannot be canceled.</h2>
</hgroup>

