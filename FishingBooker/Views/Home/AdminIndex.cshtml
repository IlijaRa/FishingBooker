        if(reservations.IsReserved != true)
        {
            continue;
        }

        switch (Convert.ToInt32(reservations.StartDate.DayOfWeek))
        {
            case 1:
                Days[0] = Days[0] + (Convert.ToDouble(reservations.Price) * (money_flow_percentage / 100));
                break;
            case 2:
